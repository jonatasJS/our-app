{
  "expo": {
    "name": "Beba água",
    "slug": "our-app",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icons/newIconNoBG.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/icons/newIconNoBG.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5593915309329672~4135236573"
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.nextrockt.ourapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/newIcon.png",
        "backgroundColor": "#fff"
      },
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-5593915309329672~4135236573"
      }
    },
    "web": {
      "favicon": "./assets/icons/newIconNoBG.png"
    },
    "extra": {
      "eas": {
        "projectId": "d3ea7596-ed6f-4259-9cc1-08cc033cb516"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/newIconNoBG.png",
          "color": "#ffffff",
          "sounds": ["./assets/sounds/notification-sound.wav"]
        }
      ]
    ],
    "description": "Beba água"
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5593915309329672~4135236573",
    "ios_app_id": "ca-app-pub-5593915309329672~4135236573"
  }
}
